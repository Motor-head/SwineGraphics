<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<DialogPane fx:id="dialog" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mod.swinegraphics.controller.PrimaryController">
   <content>
      <HBox>
         <children>
            <TitledPane animated="false" collapsible="false" prefHeight="400.0" prefWidth="400.0" text="ANIF decompress">
               <content>
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label text="Preview" />
                        <StackPane>
                           <children>
                              <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="202.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.5" style="-fx-stroke-dash-array: 5;" width="202.0" />
                              <ImageView fx:id="anifPreview" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#browseAnif" pickOnBounds="true" preserveRatio="true">
                                 <cursor>
                                    <Cursor fx:constant="CLOSED_HAND" />
                                 </cursor></ImageView>
                              <Rectangle fx:id="anifPIRect" arcHeight="5.0" fill="#000000a6" height="202.0" stroke="#000000a6" strokeType="INSIDE" strokeWidth="0.0" width="202.0" />
                              <ProgressIndicator fx:id="anifPI" />
                           </children>
                           <VBox.margin>
                              <Insets top="-7.0" />
                           </VBox.margin>
                        </StackPane>
                        <Label fx:id="anifPathTxt" lineSpacing="1.0" text="anifPathTxt" wrapText="true" />
                        <Button fx:id="anifDownloadBtn" mnemonicParsing="false" onAction="#anifDownload" text="Download" />
                        <TextArea fx:id="anifProgressTxt" editable="false" wrapText="true" />
                     </children>
                  </VBox>
               </content>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </TitledPane>
            <TitledPane animated="false" collapsible="false" prefHeight="400.0" prefWidth="400.0" text="DXT decompress">
               <content>
                  <VBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Label text="Preview" />
                        <StackPane>
                           <children>
                              <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="202.0" stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.5" style="-fx-stroke-dash-array: 5;" width="202.0" />
                              <ImageView fx:id="dxtPreview" fitHeight="200.0" fitWidth="200.0" onMouseClicked="#browseDxt" pickOnBounds="true" preserveRatio="true">
                                 <cursor>
                                    <Cursor fx:constant="CLOSED_HAND" />
                                 </cursor></ImageView>
                              <Rectangle fx:id="dxtPIRect" arcHeight="5.0" fill="#000000a6" height="202.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="202.0" />
                              <ProgressIndicator fx:id="dxtPI" />
                           </children>
                           <VBox.margin>
                              <Insets top="-7.0" />
                           </VBox.margin>
                        </StackPane>
                        <Label fx:id="dxtPathTxt" lineSpacing="1.0" text="dxtPathTxt" wrapText="true" />
                        <Button fx:id="dxtDownloadBtn" mnemonicParsing="false" onAction="#dxtDownload" text="Download" />
                        <TextArea fx:id="dxtProgressTxt" editable="false" wrapText="true" />
                     </children>
                  </VBox>
               </content>
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </TitledPane>
         </children>
      </HBox>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
</DialogPane>
